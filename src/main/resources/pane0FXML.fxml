<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<Stage minHeight="900.0" minWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <scene>
      <Scene>
         <root>
            <BorderPane minHeight="480.0" minWidth="720.0" style="-fx-background-color: #36393f;">
               <bottom>
                  <HBox style="-fx-background-color: #202225;" BorderPane.alignment="CENTER">
                     <children>
                        <Region style="-fx-background-color: #202225;" HBox.hgrow="ALWAYS" />
                        <Label text="Pane 0" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <Region HBox.hgrow="ALWAYS" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <BorderPane.margin>
                        <Insets />
                     </BorderPane.margin>
                     <padding>
                        <Insets bottom="4.0" top="4.0" />
                     </padding>
                  </HBox>
               </bottom>
               <top>
                  <HBox spacing="10.0" style="-fx-background-color: #202225;" BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Property Viewer" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <Region HBox.hgrow="ALWAYS" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="10.0" top="5.0" />
                     </padding>
                     <BorderPane.margin>
                        <Insets />
                     </BorderPane.margin>
                  </HBox>
               </top>
               <center>
                  <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                     <children>
                        <Region VBox.vgrow="ALWAYS" />
                        <HBox>
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <ImageView>
                                 <image>
                                    <Image url="@welcomeImage.png" />
                                 </image>
                              </ImageView>
                              <Region HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="To make a booking:" textFill="WHITE" />
                              <Region HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <Button mnemonicParsing="false" prefWidth="150.0" text="Sign In" />
                              <Region HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Region>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" top="5.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox>
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="If you do not have an account:" textFill="WHITE" />
                              <Region HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <Button mnemonicParsing="false" prefWidth="150.0" text="Register" />
                              <Region HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Region>
                           </children>
                           <padding>
                              <Insets top="5.0" />
                           </padding>
                        </HBox>
                        <HBox>
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <Label text="Or:" textFill="WHITE" />
                              <Region HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Region>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" top="20.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox>
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <Button mnemonicParsing="false" prefWidth="150.0" text="Continue as Guest" />
                              <Region HBox.hgrow="ALWAYS" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="50.0" top="10.0" />
                           </VBox.margin>
                        </HBox>
                        <Region VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </center>
            </BorderPane>
         </root>
      </Scene>
   </scene>
</Stage>
